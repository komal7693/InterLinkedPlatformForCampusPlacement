<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>driver</param-name>
        <param-value>oracle.jdbc.OracleDriver</param-value>
    </context-param>
    <context-param>
        <param-name>path</param-name>
        <param-value>jdbc:oracle:thin:@//LAPTOP-9544NJE2:1521/xe</param-value>
    </context-param>
    <context-param>
        <param-name>user</param-name>
        <param-value>sihproj</param-value>
    </context-param>
    <context-param>
        <param-name>pwd</param-name>
        <param-value>hariom1234</param-value>
    </context-param>
    <listener>
        <listener-class>
            classes.MyListener
        </listener-class>
    </listener>
    <servlet>
        <servlet-name>InstituteLoginServlet</servlet-name>
        <servlet-class>ServletClasses.InstituteLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AICTELoginServlet</servlet-name>
        <servlet-class>ServletClasses.AICTELoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckServlet</servlet-name>
        <servlet-class>ServletClasses.CheckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InstituteRegstraionServlet</servlet-name>
        <servlet-class>ServletClasses.InstituteRegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionExpire</servlet-name>
        <servlet-class>ServletClasses.SessionExpire</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InstituteLoginServlet</servlet-name>
        <url-pattern>/InstituteLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AICTELoginServlet</servlet-name>
        <url-pattern>/AICTELoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckServlet</servlet-name>
        <url-pattern>/CheckServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InstituteRegstraionServlet</servlet-name>
        <url-pattern>/InstituteRegstraionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionExpire</servlet-name>
        <url-pattern>/SessionExpire</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
